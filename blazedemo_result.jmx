          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor-flight value">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">String response = prev.getResponseDataAsString();

int index = response.indexOf(&quot;name=\&quot;flight\&quot;&quot;);
if (index != -1) {
    // Go backwards to find the &apos;value&apos; part before this name
    int valueStart = response.lastIndexOf(&quot;value=\&quot;&quot;, index);
    int valueEnd = response.indexOf(&quot;\&quot;&quot;, valueStart + 7); // 7 is length of &apos;value=&quot;&apos;

    if (valueStart != -1 &amp;&amp; valueEnd != -1) {
        String flightId = response.substring(valueStart + 7, valueEnd);
        vars.put(&quot;flight_id&quot;, flightId);
        log.info(&quot;Extracted Flight ID: &quot; + flightId);
    } else {
        log.warn(&quot;Flight ID value not found.&quot;);
    }
} else {
    log.warn(&quot;Flight input not found.&quot;);
}</stringProp>
          </BeanShellPostProcessor>
