            <stringProp name="script">import org.jsoup.*
import org.jsoup.parser.*
import org.jsoup.select.*

String html = prev.getResponseDataAsString()
Document doc = Jsoup.parse(html)

// Select all rows after the header
Elements rows = doc.select(&quot;table tr:gt(0)&quot;)

double maxPrice = 0
Element selectedRow = null

for (Element row : rows) {
    Elements cols = row.select(&quot;td&quot;)
    String priceText = cols.get(5).text().replace(&quot;$&quot;, &quot;&quot;).trim()
    double price = Double.parseDouble(priceText)

    if (price &gt; maxPrice) {
        maxPrice = price
        selectedRow = row
    }
}

// If a max row is found
if (selectedRow != null) {
    Elements cols = selectedRow.select(&quot;td&quot;)

    String flight = cols.get(1).text()
    String airline = cols.get(2).text()
    String price = cols.get(5).text().replace(&quot;$&quot;, &quot;&quot;)
    String fromPort = &quot;Philadelphia&quot;
    String toPort = &quot;Rome&quot;

    vars.put(&quot;selectedFlight&quot;, flight)
    vars.put(&quot;selectedPrice&quot;, price)
    vars.put(&quot;selectedAirline&quot;, airline)
    vars.put(&quot;selectedFromPort&quot;, fromPort)
    vars.put(&quot;selectedToPort&quot;, toPort)

    log.info(&quot;üî• Max Price Flight Selected:&quot;)
    log.info(&quot;Flight#: &quot; + flight)
    log.info(&quot;Airline: &quot; + airline)
    log.info(&quot;Price: $&quot; + price)
    log.info(&quot;From: &quot; + fromPort + &quot; ‚Üí To: &quot; + toPort)
} else {
    log.error(&quot;‚ùå No flight rows found!&quot;)
}
</stringProp>
