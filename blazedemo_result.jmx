          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor-flight from and to">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import java.util.regex.*;

String input = prev.getResponseDataAsString(); 

Pattern pattern = Pattern.compile(&quot;Flights from (.*?) to (.*?):&quot;);
Matcher matcher = pattern.matcher(input);

if (matcher.find()) {
    String city1 = matcher.group(1);  
    String city2 = matcher.group(2);  

    // Store in JMeter variables
    vars.put(&quot;city1&quot;, city1);
    vars.put(&quot;city2&quot;, city2);

    log.info(&quot;City 1: &quot; + city1);
    log.info(&quot;City 2: &quot; + city2);
}</stringProp>
