// Select random cities
def fromCity = departureCities[new Random().nextInt(departureCities.size())]
def toCity

// Ensure fromCity != toCity
do {
    toCity = destinationCities[new Random().nextInt(destinationCities.size())]
} while (fromCity == toCity)

// Set JMeter variables
vars.put(&quot;fromPort&quot;, fromCity)
vars.put(&quot;toPort&quot;, toCity)
